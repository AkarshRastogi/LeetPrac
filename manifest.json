{
  "manifest_version": 3,
  "name": "LeetCode Practice Reminder",
  "version": "1.0",
  "description": "Set reminders to practice LeetCode problems",
  "permissions": [
    "storage",
    "alarms",
    "notifications"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "icons": {
    "128": "icon128.png"
  },
  "host_permissions": [
    "<all_urls>"
  ]
}